{
  "name": "i2r",
  "private": true,
  "workspaces": [
    "api",
    "web"
  ],
  "scripts": {
    "postinstall": "echo \"Root install complete\"",
    "dev:api": "npm -w api run dev",
    "dev:web": "npm -w web run dev",
    "dev": "npm-run-all -p dev:*",
    "build": "npm -w api run build && npm -w web run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5"
  }
}
